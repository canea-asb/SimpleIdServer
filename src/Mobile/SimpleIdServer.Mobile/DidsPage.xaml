        <ToolbarItem Text="Active"></ToolbarItem>
        <ToolbarItem Order="Primary" Text="Delete"></ToolbarItem>
    </ContentPage.ToolbarItems>
    <StackLayout Spacing="5">
        <Grid>
            <Frame Margin="5">
                <StackLayout Orientation="Horizontal">
                    <StackLayout Orientation="Vertical" HorizontalOptions="StartAndExpand">
                        <Label>Creation date: 26-08-2024</Label>
                        <Label>Type : did-key</Label>
                    </StackLayout>
                    <Button HorizontalOptions="End" Text="Copy"></Button>
                </StackLayout>
            </Frame>
        </Grid>
    </StackLayout>
</ContentPage>